<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="275.0" prefWidth="603.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.BookListing">
   <children>
      <Label layoutX="22.0" layoutY="14.0" prefHeight="38.0" prefWidth="82.0" text="Listing: ">
         <font>
            <Font size="22.0" />
         </font>
      </Label>
      <Label fx:id="listingTitle" layoutX="125.0" layoutY="14.0" prefHeight="38.0" prefWidth="414.0" text="Listing title">
         <font>
            <Font size="22.0" />
         </font>
      </Label>
      <DatePicker fx:id="checkIn" layoutX="125.0" layoutY="76.0" />
      <Label layoutX="14.0" layoutY="70.0" prefHeight="38.0" prefWidth="109.0" text="Check in: ">
         <font>
            <Font size="22.0" />
         </font>
      </Label>
      <DatePicker fx:id="checkOut" layoutX="414.0" layoutY="76.0" onAction="#CalculatePrice" />
      <Label layoutX="303.0" layoutY="70.0" prefHeight="38.0" prefWidth="109.0" text="Check out: ">
         <font>
            <Font size="22.0" />
         </font>
      </Label>
      <Label layoutX="14.0" layoutY="123.0" prefHeight="38.0" prefWidth="109.0" text="Total price:">
         <font>
            <Font size="22.0" />
         </font>
      </Label>
      <Label fx:id="totalPrice" layoutX="125.0" layoutY="123.0" prefHeight="38.0" prefWidth="122.0">
         <font>
            <Font size="22.0" />
         </font>
      </Label>
      <Label layoutX="247.0" layoutY="123.0" prefHeight="38.0" prefWidth="19.0" text="\$">
         <font>
            <Font size="22.0" />
         </font>
      </Label>
      <Button fx:id="payButton" layoutX="257.0" layoutY="223.0" mnemonicParsing="false" onAction="#PayButtonPressed" prefHeight="38.0" prefWidth="89.0" text="Pay" />
      <Label layoutX="325.0" layoutY="123.0" prefHeight="38.0" prefWidth="109.0" text="Your price:">
         <font>
            <Font size="22.0" />
         </font>
      </Label>
      <Label fx:id="yourPrice" layoutX="436.0" layoutY="123.0" prefHeight="38.0" prefWidth="109.0">
         <font>
            <Font size="22.0" />
         </font>
      </Label>
      <Label layoutX="555.0" layoutY="123.0" prefHeight="38.0" prefWidth="19.0" text="\$">
         <font>
            <Font size="22.0" />
         </font>
      </Label>
      <Label fx:id="errorMessage" layoutX="14.0" layoutY="185.0" prefHeight="38.0" prefWidth="577.0" textFill="RED">
         <font>
            <Font size="13.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
